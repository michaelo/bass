{
    "jobname": {
        "repository": "file:///tmp/mypipeline",
        "ref": "main",
        "exec": [
            "python3", "job.py"
        ],
        "msg-pattern": ".*",
        "poll": null,
        "x-webhook": {
            "x-ref-pattern": "main"
        }
    },
    "jobname2": {
        "repository": "file:///tmp/mypipeline2",
        "ref": "main",
        "exec": [
            "python3", "job.py"
        ],
        "tag-pattern": "v[0-9]+",
        "x-poll": {
            "interval": "1h"
        },
        "webhook": null
    },
    "bass-example1": {
        "repository": "git@github.com:michaelo/bass.git",
        "ref": "main",
        "cwd": "./testpipelines",
        "exec": [
            "python3", "job.py"
        ],
        "msg-pattern": ".*",
        "poll": null,
        "x-webhook": {
            "x-ref-pattern": "main"
        }
    }
}